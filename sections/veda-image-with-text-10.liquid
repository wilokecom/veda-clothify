              {% else %}
                {% liquid
                  assign pl = 'lifestyle-1'

                  if _placeholder
                    assign pl = _placeholder
                  endif

                  assign placeholder_class = 'veda-placeholder-svg bgc:color-gray2 ' | append: 'w:100% h:100% obf:cover'

                  if _placeholder_class
                    assign placeholder_class = 'veda-placeholder-svg bgc:color-gray2 ' | append: _placeholder_class
                  endif
                %}
                {{ pl | placeholder_svg_tag: placeholder_class }}
              {% endif %}
              {% comment %}<KẾT_THÚC_TÁCH_FILE>{% endcomment %}
            </div>
            <div
              class="p:pfs(20px,40px) d:flex fld:column jc:center"
              style="width: {{ item.content___content_width }}%"
            >
              <h4
                class="c:color-gray7 trs:0.3s fw:400 fz:14px tt:uppercase lts:1px mb:10px"
                
              >
                {{ item.content___sub_title }}
              </h4>
              <h3
                class="c:color-gray9 trs:0.3s fw:500 fz:pfs(22px,26px)"
                
              >
                {{ item.content___title }}
              </h3>
              <div
                class="c:color-gray8 trs:0.3s fz:pfs(15px,18px)"
                
              >
                {{ item.content___description }}
              </div>
              <div style="text-align: {{ "left" }}">
                {% comment %}<BẮT_ĐẦU_TÁCH_FILE_2:[section,backgroundFixed,backgroundColor,bgImage,backgroundImage,sectionPadding,containerType,containerGap,containerWidth,settingGap,settingGapVertical,colGap,rowGap,sectionSeparator,_style,_placeholder,_placeholder_class,_class,_component,_default_link,aspectRatio,borderRadius,pl,placeholder_class,item]>{% endcomment %}
                {% liquid
                  if item.content___button_variant == 'link'
                    assign default_class = 'rs d:inline-block td:none lh:1.5 fz:15px ff:font-primary fw:500 trs:0.3s '
                  else
                    assign default_class = 'rs d:inline-block td:none lh:1.5 fz:15px ff:font-primary fw:500 trs:0.3s p:13px_20px bd:none bxsh:none o:none cur:pointer '
                  endif

                  assign default_for_bottom_class = ''

                  case item.content___button_variant
                    when 'primary'
                      assign variant_class = ' bgc:color-primary c:color-light-freeze bgc:color-gray9|h c:color-light|h'
                    when 'secondary'
                      assign variant_class = ' bgc:color-secondary c:color-light-freeze bgc:color-gray9|h c:color-light|h'
                    when 'dark'
                      assign variant_class = ' bgc:color-gray9 c:color-light bgc:color-primary|h c:color-light-freeze|h'
                    when 'gray'
                      assign variant_class = ' bgc:color-gray3 c:color-gray9 bgc:color-primary|h c:color-light-freeze|h'
                    when 'light'
                      assign variant_class = ' bgc:color-light c:color-gray9 bgc:color-primary|h c:color-light-freeze|h'
                    else
                      assign variant_class = ' c:color-gray9 c:color-primary|h'
                  endcase
                %}

                <a
                  component="{{ _component }}"
                  {% if 'd:block p:6px_12px! bdrs:4px fz:13px mt:20px w:fit-content!' %}
                    class="{{ default_class }}{{ default_for_bottom_class }}{{ variant_class }} {{ 'd:block p:6px_12px! bdrs:4px fz:13px mt:20px w:fit-content!' }}"
                  {% else %}
                    class="{{ default_class }}{{ default_for_bottom_class }}{{ variant_class }}"
                  {% endif %}
                  {% if item.content___new_tab %}
                    target="_blank"
                  {% endif %}
                  
                  href="{{ item.content___button_link }}"
                >
                  {% if item.content___icon_enable and 'right' == 'left' %}
                    <span
                      class="mr:10px"
                      
                    >
                      {{ item.content___choose_icon }}
                    </span>
                  {% endif %}
                  <span>
                    {{ item.content___button_text }}
                  </span>
                  {% if item.content___icon_enable and 'right' == 'right' %}
                    <span
                      class="ml:10px"
                      
                    >
                      {{ item.content___choose_icon }}
                    </span>
                  {% endif %}
                </a>
                {% comment %}<KẾT_THÚC_TÁCH_FILE>{% endcomment %}
              </div>
            </div>
          </div>
        </div>
      {% endif %}
    {% endfor %}
  </div>
{% endcapture %}

