            assign aspectRatio = item.content___aspect_ratio
          endif

          if item.content___border_radius
            assign borderRadius = item.content___border_radius
          endif
        %}
        <div
          class="col"
          {% if section.settings.VEDA_slider_4 or section.settings.VEDA_slider_4 == 0 %}
            style="{{ colGap }}"
          {% endif %}
        >
          <div
            class="group* pos:relative z:9 bgc:color-light ov:hidden bgc:color-gray1 ov:hidden d:flex"
            {% if 'left' == 'center' %}
              style="border-radius: {{ borderRadius }}px; background-color: {{ item.background_color }};justify-content: center;"
            {% elsif 'left' == 'right' %}
              style="border-radius: {{ borderRadius }}px; background-color: {{ item.background_color }};justify-content: flex-end;"
            {% else %}
              style="border-radius: {{ borderRadius }}px; background-color: {{ item.background_color }}"
            {% endif %}
          >
            <div style="padding-top: calc({{ aspectRatio }} * 1%)"></div>
            <div class="pos:absolute t:0 l:0 w:100% h:100% z:-1">
              {% comment %}<BẮT_ĐẦU_TÁCH_FILE_1:[section,backgroundFixed,backgroundColor,bgImage,backgroundImage,sectionPadding,containerType,containerGap,containerWidth,settingGap,settingGapVertical,colGap,rowGap,sectionSeparator,_style,_placeholder,_placeholder_class,_class,_component,_default_link,aspectRatio,borderRadius,item]>{% endcomment %}
