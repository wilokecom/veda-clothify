              data-css="{{ null }}"
            >
              {% comment %}<BẮT_ĐẦU_TÁCH_FILE_3:[section,backgroundFixed,backgroundColor,bgImage,backgroundImage,sectionPadding,containerType,containerGap,containerWidth,settingGap,settingGapVertical,colGap,rowGap,sectionSeparator,_prefix,_style,_placeholder,_placeholder_class,_class,_component,_defaul_aspect_ratio,_image_border_radius,imageRadius,aspectRatio,quantity_displayed,articleCond,pageCond,productCond,articleItem,pl,placeholder_class,tagSize,excerptSize,productItem]>{% endcomment %}
              {% liquid
                assign price_compare = productItem.compare_at_price

                if price_compare
                  assign price_compare = productItem.compare_at_price
                else
                  assign price_compare = 0
                endif
              %}
              <script class="{{ 'product-card-data-js' }}" type="application/json">
                {
                  id: {{ productItem.id | json }},
                  title: {{ productItem.title | json }},
                  vendor: {{ productItem.vendor | json }},
                  handle: {{ productItem.handle | json }},
                  available: {{ productItem.available | json }},
                  content: {{ productItem.content | json }},
                  description: {{ productItem.description | json }},
                  featured_image: {{ productItem.featured_image | json }},
                  images: {{ productItem.images | json }},
                  options: {{ productItem.options | json }},
                  selected_or_first_available_variant: {
                    id: {{ productItem.selected_or_first_available_variant.id | json }},
                    available: {{ productItem.selected_or_first_available_variant.available | json }},
                    price: {{ productItem.selected_or_first_available_variant.price | money | json }},
                    {% if productItem.selected_or_first_available_variant.compare_at_price %}
                      compare_at_price: {{ productItem.selected_or_first_available_variant.compare_at_price | money | json }},
                    {% else %}
                      compare_at_price: {{ 0 | money | json }},
                    {% endif %}
                    featured_image: {{ productItem.selected_or_first_available_variant.featured_image | json }},
                    image: {{ productItem.selected_or_first_available_variant.image | json }},
                    inventory_quantity: {{ productItem.selected_or_first_available_variant.inventory_quantity | json }},
                    options: {{ productItem.selected_or_first_available_variant.options | json }},
                    title: {{ productItem.selected_or_first_available_variant.title | json }},
                    sku: {{ productItem.selected_or_first_available_variant.sku | json }},
                    url: {{ productItem.selected_or_first_available_variant.url | json }},
                  },
                  price_max: {{ productItem.price_max | json }},
                  price_min: {{ productItem.price_min | json }},
                  type: {{ productItem.type | json }},
                  tags: {{ productItem.tags | json }},
                  variants: [
                  {% for item in productItem.variants %}
                      {
                        id: {{ item.id | json }},
                        available: {{ item.available | json }},
                        price: {{ item.price | money | json }},
                        {% if item.compare_at_price %}
                          compare_at_price: {{ item.compare_at_price | money | json }},
                        {% else %}
                          compare_at_price: {{ 0 | money | json }},
                        {% endif %}
                        featured_image: {{ item.featured_image | json }},
                        image: {{ item.image | json }},
                        inventory_quantity: {{ item.inventory_quantity | json }},
                        options: {{ item.options | json }},
                        title: {{ item.title | json }},
                        sku: {{ item.sku | json }},
                        url: {{ item.url | json }},
                      },
                  {% endfor %}
                ],
                  price: {{ productItem.price | money | json }},
                  compare_at_price: {{ price_compare | money | json }},
                  options_with_values: {{ productItem.options_with_values | json }},
                  url: {{ productItem.url | json }},
                  published_at: {{ productItem.published_at | json }}
                }
              </script>
              {% comment %}<KẾT_THÚC_TÁCH_FILE>{% endcomment %}
              <script type="application/json" class="quickview-data-js">
                {
                  add_to_cart_text: "{{ 'veda.products.product.add_to_cart' | t }}",
                  quantity_text: "{{ 'veda.products.product.quantity.label' | t }}",
                  sold_out_text: "{{ 'veda.products.product.sold_out' | t }}",
                  view_detail: "{{ 'veda.products.product.view_details' | t }}",
                  add_compare: "{{ 'veda.products.product.add_to_compare' | t }}",
                  remove_compare: "{{ 'veda.products.product.remove_from_compare' | t }}",
                  add_wish_list: "{{ 'veda.products.product.add_to_wishlist' | t }}",
                  remove_wish_list: "{{ 'veda.products.product.remove_from_wishlist' | t }}"
                }
              </script>
              <script type="application/json" class="current-variant-id">
                {{ productItem.selected_or_first_available_variant.id | json }}
              </script>
              <div class="pos:absolute t:10px l:10px z:12 d:flex fld:column">
                {% comment %}<BẮT_ĐẦU_TÁCH_FILE_4:[section,backgroundFixed,backgroundColor,bgImage,backgroundImage,sectionPadding,containerType,containerGap,containerWidth,settingGap,settingGapVertical,colGap,rowGap,sectionSeparator,_prefix,_style,_placeholder,_placeholder_class,_class,_component,_defaul_aspect_ratio,_image_border_radius,imageRadius,aspectRatio,quantity_displayed,articleCond,pageCond,productCond,articleItem,pl,placeholder_class,tagSize,excerptSize,productItem,price_compare]>{% endcomment %}
                {% liquid
                  assign productDate = productItem.published_at | date: '%s'

                  assign currentDate = 'now' | date: '%s'

                  assign difference = currentDate | minus: productDate | divided_by: 86400.0
                %}
                {% if section.settings.new___enable %}
                  {% if difference < 10 %}
                    <div
                      {% if 'p:3px_10px bdrs:3px bgc:#17c478 lh:normal fz:12px ff:font-secondary tt:uppercase lts:1px c:color-light-freeze mb:5px' %}
                        class="w:fit-content {{ 'p:3px_10px bdrs:3px bgc:#17c478 lh:normal fz:12px ff:font-secondary tt:uppercase lts:1px c:color-light-freeze mb:5px' }}"
