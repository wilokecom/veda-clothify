                      {% if 'new' %}
                        component="{{ 'new' }}"
                      {% endif %}
                      data-css="{{ null }}"
                    >
                      {{ 'veda.products.product.new' | t }}
                    </div>
                  {% endif %}
                {% endif %}
                {% comment %}<KẾT_THÚC_TÁCH_FILE>{% endcomment %}
                {% comment %}<BẮT_ĐẦU_TÁCH_FILE_5:[section,backgroundFixed,backgroundColor,bgImage,backgroundImage,sectionPadding,containerType,containerGap,containerWidth,settingGap,settingGapVertical,colGap,rowGap,sectionSeparator,_prefix,_style,_placeholder,_placeholder_class,_class,_component,_defaul_aspect_ratio,_image_border_radius,imageRadius,aspectRatio,quantity_displayed,articleCond,pageCond,productCond,articleItem,pl,placeholder_class,tagSize,excerptSize,productItem,price_compare,productDate,currentDate,difference]>{% endcomment %}
                {% if productItem.compare_at_price > productItem.price and section.settings.sale___enable %}
                  <div
                    {% if 'p:3px_10px bdrs:3px bgc:#eb4747 lh:normal fz:12px ff:font-secondary tt:uppercase lts:1px c:color-light-freeze' %}
                      class="w:fit-content {{ 'p:3px_10px bdrs:3px bgc:#eb4747 lh:normal fz:12px ff:font-secondary tt:uppercase lts:1px c:color-light-freeze' }}"
                    {% else %}
                      class="p:5px_10px bdrs:3px bgc:#eb4747 lh:normal fz:10px ff:font-secondary tt:uppercase lts:1px c:color-light-freeze w:fit-content"
                    {% endif %}
                    {% if 'sale' %}
                      component="{{ 'sale' }}"
                    {% endif %}
                    data-css="{{ null }}"
                  >
                    {% liquid
                      assign ratio = productItem.price | times: 100.0 | divided_by: productItem.compare_at_price
                    %}
                    -{{ 100 | minus: ratio | ceil }}%
                  </div>
                {% endif %}
                {% comment %}<KẾT_THÚC_TÁCH_FILE>{% endcomment %}
              </div>
              <div class="w:100% pos:relative">
                {% comment %}<BẮT_ĐẦU_TÁCH_FILE_6:[section,backgroundFixed,backgroundColor,bgImage,backgroundImage,sectionPadding,containerType,containerGap,containerWidth,settingGap,settingGapVertical,colGap,rowGap,sectionSeparator,_prefix,_style,_placeholder,_placeholder_class,_class,_component,_defaul_aspect_ratio,_image_border_radius,imageRadius,aspectRatio,quantity_displayed,articleCond,pageCond,productCond,articleItem,pl,placeholder_class,tagSize,excerptSize,productItem,price_compare,productDate,currentDate,difference,ratio]>{% endcomment %}
                {% liquid
                  if _defaul_aspect_ratio and _defaul_aspect_ratio != 0
                    assign aspectRatio = _defaul_aspect_ratio
                  else
                    assign aspectRatio = 90
                  endif

                  if section.settings.VEDA_slider_3 and section.settings.VEDA_slider_3 != 0
                    assign aspectRatio = section.settings.VEDA_slider_3
                  endif
                %}
                <div
