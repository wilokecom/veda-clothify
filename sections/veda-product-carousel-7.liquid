{% assign uniqueId = 'id_b76d56df-68f4-4004-8992-92a17a58e6e1' %}
{% assign builderMode = false %}
{% assign envatoMode = true %}
{% liquid
  if section.settings.VEDA_switch_11
    assign backgroundFixed = 'fixed'
  else
    assign backgroundFixed = 'scroll'
  endif

  if section.settings.VEDA_select_6 == 'color'
    assign opacity = section.settings.background_opacity | times: 0.01
    assign modifyBackground = section.settings.background_color | color_modify: 'alpha', opacity
    assign backgroundColor = 'background-color: ' | append: modifyBackground | append: ';'
  else
    assign backgroundColor = ''
  endif

  if section.settings.VEDA_select_6 == 'image' and section.settings.VEDA_image_1.src
    assign bgImage = section.settings.VEDA_image_1.src | image_url: width: 2000

    assign backgroundImage = 'background-image: url(' | append: bgImage | append: ');background-attachment: ' | append: backgroundFixed | append: ';background-size: ' | append: section.settings.VEDA_select_5 | append: ';'
  elsif section.settings.VEDA_select_6 == 'image' and section.settings.VEDA_image_1.src == blank
    assign bgImage = 'https://cdn.shopify.com/s/files/1/0552/5510/5616/files/udxhtnzxtq.png?v=1681903122&width=1000'

    assign backgroundImage = 'background-image: url(' | append: bgImage | append: ');background-attachment: ' | append: backgroundFixed | append: ';background-size: ' | append: section.settings.VEDA_select_5 | append: ';'
  else
    assign backgroundImage = ''
  endif

  assign sectionPadding = 'padding-top: ' | append: 50 | append: 'px;padding-right: ' | append: null | append: 'px;padding-bottom: ' | append: 50 | append: 'px;padding-left: ' | append: null | append: 'px;'

  if section.settings.VEDA_select_7 == 'full_width'
    assign containerType = 'container-fluid'
  else
    assign containerType = 'container'
  endif

  if section.settings.VEDA_slider_9 >= 0
    assign containerGap = 'padding-left: ' | append: section.settings.VEDA_slider_9 | append: 'px; padding-right: ' | append: section.settings.VEDA_slider_9 | append: 'px;'
  else
    assign containerGap = ''
  endif

  if section.settings.section_settings___width >= 0
    assign containerWidth = 'max-width: ' | append: section.settings.section_settings___width | append: 'px;'
  else
    assign containerWidth = ''
  endif

  if section.settings.VEDA_slider_8 >= 0
    assign settingGap = section.settings.VEDA_slider_8 | divided_by: 2.0

    assign settingGapVertical = section.settings.VEDA_slider_8
  else
    assign settingGap = ''

    assign settingGapVertical = ''
  endif

  assign colGap = 'margin-top: ' | append: settingGapVertical | append: 'px; padding-left: ' | append: settingGap | append: 'px; padding-right: ' | append: settingGap | append: 'px;'

  assign rowGap = 'margin-top: -' | append: settingGapVertical | append: 'px; margin-left: -' | append: settingGap | append: 'px; margin-right: -' | append: settingGap | append: 'px;'

  assign sectionSeparator = ''

  if section.settings.VEDA_switch_9
    if section.settings.VEDA_slider_6
      assign sectionSeparator = sectionSeparator | append: 'border-top: ' | append: section.settings.VEDA_slider_6 | append: 'px solid ' | append: '#f3f3f3' | append: ';'
    endif

    if section.settings.VEDA_slider_5
      assign sectionSeparator = sectionSeparator | append: 'border-bottom: ' | append: section.settings.VEDA_slider_5 | append: 'px solid ' | append: '#f3f3f3' | append: ';'
    endif
  endif

  if colGap and rowGap and builderMode and envatoMode
  endif
%}

{% liquid
  assign _class = false

  assign _component = false

  assign _defaul_aspect_ratio = false

  assign _image_border_radius = false

  assign _default_link = false

  assign _alt = false

  assign _style = false

  assign _placeholder = false

  assign _placeholder_class = false
%}
{% capture Content %}
  {% liquid
    assign quantity_product_displayed = section.settings.product_settings___limit

    assign collectionData = collections[section.settings.VEDA_collectionPicker_1]

    assign productData = collectionData.products
  %}
  <script type="application/json" class="quickview-data-js">
    {
      add_to_cart_text: "{{ 'veda.products.product.add_to_cart' | t }}",
      quantity_text: "{{ 'veda.products.product.quantity.label' | t }}",
      sold_out_text: "{{ 'veda.products.product.sold_out' | t }}",
      view_detail: "{{ 'veda.products.product.view_details' | t }}",
      add_compare: "{{ 'veda.products.product.add_to_compare' | t }}",
      remove_compare: "{{ 'veda.products.product.remove_from_compare' | t }}",
      add_wish_list: "{{ 'veda.products.product.add_to_wishlist' | t }}",
      remove_wish_list: "{{ 'veda.products.product.remove_from_wishlist' | t }}"
    }
  </script>
  {% if collectionData != blank %}
    {% for productItem in productData limit: quantity_product_displayed %}
      <div
        class="swiper-slide w:var(--width-xs)@+sm d:flex fld:column mih:100%"
        style="--width-xs: calc({{ 100 | divided_by: 1 }}% - {{ 40 | divided_by: 2 }}px);"
      >
        <div
          class="product-card-js group* pos:relative mih:100%"
          data-css="{{ null }}"
        >
          {% comment %}<BẮT_ĐẦU_TÁCH_FILE_1:[section,backgroundFixed,backgroundColor,bgImage,backgroundImage,sectionPadding,containerType,containerGap,containerWidth,settingGap,settingGapVertical,colGap,rowGap,sectionSeparator,_class,_component,_defaul_aspect_ratio,_image_border_radius,_default_link,_alt,_style,_placeholder,_placeholder_class,quantity_product_displayed,collectionData,productData]>{% endcomment %}
          {% liquid
            assign price_compare = productItem.compare_at_price

            if price_compare
              assign price_compare = productItem.compare_at_price
            else
              assign price_compare = 0
            endif
          %}
          <script class="{{ 'product-card-data-js' }}" type="application/json">
            {
              id: {{ productItem.id | json }},
              title: {{ productItem.title | json }},
              vendor: {{ productItem.vendor | json }},
              handle: {{ productItem.handle | json }},
              available: {{ productItem.available | json }},
              content: {{ productItem.content | json }},
              description: {{ productItem.description | json }},
              featured_image: {{ productItem.featured_image | json }},
              images: {{ productItem.images | json }},
              options: {{ productItem.options | json }},
              selected_or_first_available_variant: {
                id: {{ productItem.selected_or_first_available_variant.id | json }},
                available: {{ productItem.selected_or_first_available_variant.available | json }},
                price: {{ productItem.selected_or_first_available_variant.price | money | json }},
                {% if productItem.selected_or_first_available_variant.compare_at_price %}
                  compare_at_price: {{ productItem.selected_or_first_available_variant.compare_at_price | money | json }},
                {% else %}
                  compare_at_price: {{ 0 | money | json }},
                {% endif %}
                featured_image: {{ productItem.selected_or_first_available_variant.featured_image | json }},
                image: {{ productItem.selected_or_first_available_variant.image | json }},
                inventory_quantity: {{ productItem.selected_or_first_available_variant.inventory_quantity | json }},
                options: {{ productItem.selected_or_first_available_variant.options | json }},
                title: {{ productItem.selected_or_first_available_variant.title | json }},
                sku: {{ productItem.selected_or_first_available_variant.sku | json }},
                url: {{ productItem.selected_or_first_available_variant.url | json }},
              },
              price_max: {{ productItem.price_max | json }},
              price_min: {{ productItem.price_min | json }},
              type: {{ productItem.type | json }},
              tags: {{ productItem.tags | json }},
              variants: [
              {% for item in productItem.variants %}
                  {
                    id: {{ item.id | json }},
                    available: {{ item.available | json }},
                    price: {{ item.price | money | json }},
                    {% if item.compare_at_price %}
                      compare_at_price: {{ item.compare_at_price | money | json }},
                    {% else %}
                      compare_at_price: {{ 0 | money | json }},
                    {% endif %}
                    featured_image: {{ item.featured_image | json }},
                    image: {{ item.image | json }},
                    inventory_quantity: {{ item.inventory_quantity | json }},
                    options: {{ item.options | json }},
                    title: {{ item.title | json }},
                    sku: {{ item.sku | json }},
                    url: {{ item.url | json }},
                  },
              {% endfor %}
            ],
              price: {{ productItem.price | money | json }},
              compare_at_price: {{ price_compare | money | json }},
              options_with_values: {{ productItem.options_with_values | json }},
              url: {{ productItem.url | json }},
              published_at: {{ productItem.published_at | json }}
            }
          </script>
          {% comment %}<KẾT_THÚC_TÁCH_FILE>{% endcomment %}

          <script type="application/json" class="current-variant-id">
            {{ productItem.selected_or_first_available_variant.id | json }}
          </script>
          <div class="pos:absolute t:10px l:10px z:12 d:flex fld:column">
            {% comment %}<BẮT_ĐẦU_TÁCH_FILE_2:[section,backgroundFixed,backgroundColor,bgImage,backgroundImage,sectionPadding,containerType,containerGap,containerWidth,settingGap,settingGapVertical,colGap,rowGap,sectionSeparator,_class,_component,_defaul_aspect_ratio,_image_border_radius,_default_link,_alt,_style,_placeholder,_placeholder_class,quantity_product_displayed,collectionData,productData,price_compare]>{% endcomment %}
            {% liquid
              assign productDate = productItem.published_at | date: '%s'

              assign currentDate = 'now' | date: '%s'

              assign difference = currentDate | minus: productDate | divided_by: 86400.0
            %}
            {% if section.settings.new___enable %}
              {% if difference < 10 %}
                <div
                  {% if 'p:3px_10px bdrs:3px bgc:#17c478 lh:normal fz:12px ff:font-secondary tt:uppercase lts:1px c:color-light-freeze mb:5px' %}
                    class="w:fit-content {{ 'p:3px_10px bdrs:3px bgc:#17c478 lh:normal fz:12px ff:font-secondary tt:uppercase lts:1px c:color-light-freeze mb:5px' }}"
                  {% else %}
                    class="p:5px_10px bdrs:3px bgc:#17c478 lh:normal fz:10px ff:font-secondary tt:uppercase lts:1px c:color-light-freeze mb:5px w:fit-content"
                  {% endif %}
                  {% if 'new' %}
                    component="{{ 'new' }}"
                  {% endif %}
                  data-css="{{ null }}"
                >
                  {{ 'veda.products.product.new' | t }}
                </div>
              {% endif %}
            {% endif %}
            {% comment %}<KẾT_THÚC_TÁCH_FILE>{% endcomment %}
            {% comment %}<BẮT_ĐẦU_TÁCH_FILE_3:[section,backgroundFixed,backgroundColor,bgImage,backgroundImage,sectionPadding,containerType,containerGap,containerWidth,settingGap,settingGapVertical,colGap,rowGap,sectionSeparator,_class,_component,_defaul_aspect_ratio,_image_border_radius,_default_link,_alt,_style,_placeholder,_placeholder_class,quantity_product_displayed,collectionData,productData,price_compare,productDate,currentDate,difference]>{% endcomment %}
            {% if productItem.compare_at_price > productItem.price and section.settings.sale___enable %}
              <div
                {% if 'p:3px_10px bdrs:3px bgc:#eb4747 lh:normal fz:12px ff:font-secondary tt:uppercase lts:1px c:color-light-freeze' %}
                  class="w:fit-content {{ 'p:3px_10px bdrs:3px bgc:#eb4747 lh:normal fz:12px ff:font-secondary tt:uppercase lts:1px c:color-light-freeze' }}"
                {% else %}
                  class="p:5px_10px bdrs:3px bgc:#eb4747 lh:normal fz:10px ff:font-secondary tt:uppercase lts:1px c:color-light-freeze w:fit-content"
                {% endif %}
                {% if 'sale' %}
                  component="{{ 'sale' }}"
                {% endif %}
                data-css="{{ null }}"
              >
                {% liquid
                  assign ratio = productItem.price | times: 100.0 | divided_by: productItem.compare_at_price
                %}
                -{{ 100 | minus: ratio | ceil }}%
              </div>
            {% endif %}
            {% comment %}<KẾT_THÚC_TÁCH_FILE>{% endcomment %}
          </div>
          <div class="w:100% pos:relative">
            {% comment %}<BẮT_ĐẦU_TÁCH_FILE_4:[section,backgroundFixed,backgroundColor,bgImage,backgroundImage,sectionPadding,containerType,containerGap,containerWidth,settingGap,settingGapVertical,colGap,rowGap,sectionSeparator,_class,_component,_defaul_aspect_ratio,_image_border_radius,_default_link,_alt,_style,_placeholder,_placeholder_class,quantity_product_displayed,collectionData,productData,price_compare,productDate,currentDate,difference,ratio]>{% endcomment %}
            {% liquid
              if _defaul_aspect_ratio and _defaul_aspect_ratio != 0
                assign aspectRatio = _defaul_aspect_ratio
              else
                assign aspectRatio = 90
              endif

              if section.settings.VEDA_slider_4 and section.settings.VEDA_slider_4 != 0
                assign aspectRatio = section.settings.VEDA_slider_4
              endif
            %}
            <div
              class="ov:hidden w:100%"
              {% if _image_border_radius %}
                style="border-radius: {{ _image_border_radius }}px"
              {% endif %}
            >
              <a
                href="{{ productItem.url }}"
                class="veda-image-cover pos:relative d:block trs:opacity_0.3s z:1 {% if productItem.images[1] %}op:0*group-hover{% endif %}"
                style="--aspect-ratio:{{ aspectRatio }}"
              >
                <img
                  class="product-card-image-js veda-lazyload"
                  src="{{ productItem.featured_image.src | image_url: width: 10 }}"
                  alt="{{ productItem.title }}"
                  width="{{ productItem.featured_image.width }}"
                  height="{{ productItem.featured_image.height }}"
                  data-intrinsic-width="{{ productItem.featured_image.width }}"
                  data-intrinsic-height="{{ productItem.featured_image.height }}"
                  loading="lazy"
                >
              </a>
              {% if productItem.images[1] %}
                <a
                  href="{{ productItem.url }}"
                  class="veda-image-cover pos:absolute! t:0 l:-100000px w:100% z:9 op:0 trs:opacity_0.4s,left_0s_0.4s trs:opacity_0.4s,left_0s_0s*group-hover op:1*group-hover l:0*group-hover"
                  style="--aspect-ratio:{{ aspectRatio }}"
                >
                  <img
                    class="veda-lazyload bgc:color-light-freeze"
                    src="{{ productItem.images[1].src | image_url: width: 10 }}"
                    alt="{{ productItem.title }}"
                    width="{{ productItem.featured_image.width }}"
                    height="{{ productItem.featured_image.height }}"
                    data-intrinsic-width="{{ productItem.images[1].width }}"
                    data-intrinsic-height="{{ productItem.images[1].height }}"
                    loading="lazy"
                  >
                </a>
              {% endif %}
            </div>
            {% comment %}<KẾT_THÚC_TÁCH_FILE>{% endcomment %}
            {% comment %}<BẮT_ĐẦU_TÁCH_FILE_5:[section,backgroundFixed,backgroundColor,bgImage,backgroundImage,sectionPadding,containerType,containerGap,containerWidth,settingGap,settingGapVertical,colGap,rowGap,sectionSeparator,_class,_component,_defaul_aspect_ratio,_image_border_radius,_default_link,_alt,_style,_placeholder,_placeholder_class,quantity_product_displayed,collectionData,productData,price_compare,productDate,currentDate,difference,ratio,aspectRatio]>{% endcomment %}
            <div class="pos:absolute l:0 b:0 r:0 z:12 h:50px m:auto ov:hidden ov:visible|h">
              <div class="pos:absolute l:10px b:10px b:-5px@md r:10px h:34px h:38px@md d:flex cur:pointer trf:translateY(100%)@md trs:0.3s trf:translateY(0)*group-hover b:10px*group-hover">
                {% if section.settings.quick_view___enable %}
                  <span
                    class="quickview-toggle-js mr:5px w:34px w:38px@md h:100% pos:relative fz:14px bgc:color-gray9-freeze c:color-light-freeze trs:0.3s c:color-primary|h d:flex! ai:center jc:center bdrs:4px|be fz:12px|be"
                    data-tooltip-text="{{ 'veda.products.product.quick_view' | t }}"
                    data-tooltip-position="top"
                    data-tooltip-active="false"
                    data-css="{{ null }}"
                  >
                    <i class="far fa-eye"></i>
                  </span>
                {% endif %}
                <span
                  class="product-card-add-js lines-1 w:34px@+md flx:1@md h:100% pos:relative fz:14px ff:font-secondary bgc:color-gray9-freeze c:color-light-freeze trs:0.3s c:color-primary|h d:flex! ai:center jc:center us:none"
                  data-tooltip-position="top"
                  data-tooltip-text="{{ 'veda.products.product.add_to_cart' | t }}"
                  data-cart-success-text="{{ 'veda.products.product.add_to_cart' | t }}"
                  data-css="{{ null }}"
                >
                  <div class="product-cart-add-icon-js mr:10px@md"><i class="far fa-shopping-cart"></i></div>
                  <span class="d:none@+md">{{ 'veda.products.product.add_to_cart' | t }}</span>
                </span>
              </div>
            </div>
            <div class="pos:absolute t:0 r:0 b:0 z:12 w:44px m:auto ov:hidden ov:visible|h">
              <div class="pos:absolute r:10px r:-5px@md t:50%@md t:10px w:34px d:flex fld:column ai:center cur:pointer trf:translate(100%,-50%)@md trs:0.3s trf:translate(0,-50%)*group-hover@md r:10px*group-hover@md">
                {% if section.settings.VEDA_switch_4 %}
                  <span
                    class="wishlist-toggle-js mb:8px w:34px h:34px pos:relative fz:14px bdrs:50% bgc:color-light c:color-gray9 trs:0.3s c:color-primary|h d:flex! ai:center jc:center bdrs:4px|be fz:12px|be"
                    data-tooltip-position="left"
                    data-tooltip-active="true"
                    data-tooltip-text="{{ 'veda.products.product.add_to_wishlist' | t }}"
                    data-tooltip-active-text="{{ 'veda.products.product.remove_from_wishlist' | t }}"
                    data-css="{{ null }}"
                  >
                    <i class="fas fa-heart"></i>
                  </span>
                {% endif %}
                {% if section.settings.compare_action___enable %}
                  <span
                    class="compare-toggle-js w:34px h:34px pos:relative fz:14px bdrs:50% bgc:color-light c:color-gray9 trs:0.3s c:color-primary|h d:flex! ai:center jc:center bdrs:4px|be fz:12px|be"
                    data-tooltip-position="left"
                    data-tooltip-active="false"
                    data-tooltip-text="{{ 'veda.products.product.add_to_compare' | t }}"
                    data-tooltip-active-text="{{ 'veda.products.product.remove_from_compare' | t }}"
                    data-css="{{ null }}"
                  >
                    <i class="fas fa-repeat-alt"></i>
                  </span>
                {% endif %}
              </div>
            </div>
            {% comment %}<KẾT_THÚC_TÁCH_FILE>{% endcomment %}

            {% comment %}<BẮT_ĐẦU_TÁCH_FILE_6:[section,backgroundFixed,backgroundColor,bgImage,backgroundImage,sectionPadding,containerType,containerGap,containerWidth,settingGap,settingGapVertical,colGap,rowGap,sectionSeparator,_class,_component,_defaul_aspect_ratio,_image_border_radius,_default_link,_alt,_style,_placeholder,_placeholder_class,quantity_product_displayed,collectionData,productData,price_compare,productDate,currentDate,difference,ratio,aspectRatio]>{% endcomment %}
            {% liquid
              assign is_sold_out = true

              assign variant = productItem.selected_or_first_available_variant

              if variant.available
                assign is_sold_out = false
              endif

              if builderMode
                assign poiter_event = ''
              else
                assign poiter_event = 'pe:none'
              endif

              if is_sold_out and section.settings.sold_out___enable
                assign visibleSoldOut = 'd:block'
              else
                assign visibleSoldOut = 'd:none'
              endif
            %}
            <div class="sold-out-js pos:absolute t:50% l:0 p:pfs(5px,20px) w:100% h:fit-content trf:translateY(-50%) bg:transparent!*span ta:center z:10 {{ poiter_event }} {{ visibleSoldOut }}">
              <div
                class="pos:absolute t:0 l:0 w:100% h:100% bgc:color-gray9 op:0.4 z:-1"
                data-css="{{ null }}"
              ></div>
              <span
                class=" fz:pfs(13px,20px) fw:500 c:color-light"
                style="background: transparent !important; background-color: transparent !important"
                data-css="{{ null }}"
              >
                {{ 'veda.products.product.sold_out' | t }}
              </span>
            </div>
            {% comment %}<KẾT_THÚC_TÁCH_FILE>{% endcomment %}
          </div>
          <div
            class="d:flex fld:column pt:10px"
            style="text-align: {{ "left" }}"
          >
            <div style="order:{{ "0" }}">
              {% comment %}<BẮT_ĐẦU_TÁCH_FILE_7:[section,backgroundFixed,backgroundColor,bgImage,backgroundImage,sectionPadding,containerType,containerGap,containerWidth,settingGap,settingGapVertical,colGap,rowGap,sectionSeparator,_class,_component,_defaul_aspect_ratio,_image_border_radius,_default_link,_alt,_style,_placeholder,_placeholder_class,quantity_product_displayed,collectionData,productData,price_compare,productDate,currentDate,difference,ratio,aspectRatio,is_sold_out,variant,poiter_event,visibleSoldOut]>{% endcomment %}
              {% if section.settings.VEDA_switch_8 %}
                <a
                  class="{{ 'd:block fz:12px ff:font-secondary c:color-gray7' }}"
                  data-css="{{ null }}"
                  href="{{ shop.url }}/collections/vendors?q={{ productItem.vendor }}"
                  {% if _component %}
                    component="{{ _component }}"
                  {% endif %}
                >
                  {{ productItem.vendor }}
                </a>
              {% endif %}
              {% comment %}<KẾT_THÚC_TÁCH_FILE>{% endcomment %}
            </div>
            <h3 style="order:{{ "1" }}">
              <a
                class="d:block mt:5px fz:14px ff:font-secondary fw:400 c:color-gray9 trs:0.3s"
                href="{{ productItem.url }}"
                data-css="{{ null }}"
              >
                {{ productItem.title }}
              </a>
            </h3>
            <div class="compare-rating-js" style="order:1">
              {% case settings.theme_review_app %}
                {% when 'shopify' %}
                  <span class="shopify-product-reviews-badge" data-id="{{ productItem.id }}"></span>
                {% when 'judgeme' %}
                  {% render 'judgeme_core' %}
                    {% render 'judgeme_widgets',
                      widget_type: 'judgeme_preview_badge',
                      jm_style: 'font-size: 16px;font-weight: 400;',
                      concierge_install: false,
                      product: productItem
                    %}
                {% when 'ryviu' %}
                  <div class="ryviu-collection">
                    <ryviu-widget-total
                      collection="1"
                      product_id="{{ productItem.id }}"
                      handle="{{ productItem.handle }}"
                      reviews_data="{{ productItem.metafields.ryviu.product_reviews_info | escape }}"
                    ></ryviu-widget-total>
                  </div>
                {% when 'stamped' %}
                  <span
                    class="stamped-product-reviews-badge stamped-main-badge"
                    data-id="{{ productItem.id }}"
                    data-product-sku="{{ productItem.handle }}"
                    data-product-title="{{ productItem.title }}"
                    data-product-type="{{ productItem.type }}"
                    style="display: block;"
                  >
                    {{ productItem.metafields.stamped.badge }}
                  </span>
                {% when 'rivyo' %}
                  <div
                    class="wc_product_review_badge"
                    data-handle="{{ productItem.handle }}"
                    data-product_id="{{ productItem.id }}"
                  ></div>
                {% when 'ali' %}
                    <div
                      product-id="{{ productItem.id }}"
                      product-handle="{{ productItem.handle }}"
                      class="alireviews-review-star-rating"
                    ></div>
                {% when 'areviews' %}
                  <div
                    class="areviews_product_item areviews_stars{{ productItem.id }}"
                    data-product-id="{{ productItem.id }}"
                  ></div>
                {% when 'loox' %}
                  <div
                    class="loox-rating"
                    data-id="{{ productItem.id }}"
                    data-rating="{{ productItem.metafields.loox.avg_rating }}"
                    data-raters="{{ productItem.metafields.loox.num_reviews }}"
                  ></div>
                {% when 'lai' %}
                  <div
                    class="scm-reviews-rate"
                    data-rate-version2="{{ productItem.metafields.scm_review_importer.reviewsData.reviewCountInfo | json }}"
                    data-product-id="{{ productItem.id }}"
                  ></div>
                  <script>
                    if(typeof SMARTIFYAPPS!== 'undefined' && SMARTIFYAPPS.rv.installed){
                    SMARTIFYAPPS.rv.scmReviewsRate.actionCreateReviews(); }
