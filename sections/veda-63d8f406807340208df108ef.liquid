{% assign uniqueId = 'id_5aa720ba-d3b6-43be-9319-cb5cb53394ed' %}
{% assign builderMode = false %}
{% assign envatoMode = true %}
{% comment %}<BẮT_ĐẦU_TÁCH_FILE_1:[section]>{% endcomment %}
{% liquid
  if null
    assign backgroundFixed = 'fixed'
  else
    assign backgroundFixed = 'scroll'
  endif

  if 'none' == 'color'
    assign backgroundColor = 'background-color: ' | append: null | append: ';'
  else
    assign backgroundColor = ''
  endif

  if 'none' == 'image' and section.settings.VEDA_image_1.src
    assign bgImage = section.settings.VEDA_image_1.src | image_url: width: 2000

    assign backgroundImage = 'background-image: url(' | append: bgImage | append: ');background-attachment: ' | append: backgroundFixed | append: ';background-size: ' | append: 'cover' | append: ';'
  elsif 'none' == 'image' and section.settings.VEDA_image_1.src == blank
    assign bgImage = 'https://cdn.shopify.com/s/files/1/0552/5510/5616/files/udxhtnzxtq.png?v=1681903122&width=1000'

    assign backgroundImage = 'background-image: url(' | append: bgImage | append: ');background-attachment: ' | append: backgroundFixed | append: ';background-size: ' | append: 'cover' | append: ';'
  else
    assign backgroundImage = ''
  endif

  assign sectionPadding = 'padding-top: ' | append: 50 | append: 'px;padding-right: ' | append: null | append: 'px;padding-bottom: ' | append: 50 | append: 'px;padding-left: ' | append: null | append: 'px;'

  if section.settings.VEDA_select_3 == 'full_width'
    assign containerType = 'container-fluid'
  else
    assign containerType = 'container'
  endif

  if section.settings.VEDA_slider_6 >= 0
    assign containerGap = 'padding-left: ' | append: section.settings.VEDA_slider_6 | append: 'px; padding-right: ' | append: section.settings.VEDA_slider_6 | append: 'px;'
  else
    assign containerGap = ''
  endif

  if section.settings.section_settings___width >= 0
    assign containerWidth = 'max-width: ' | append: section.settings.section_settings___width | append: 'px;'
  else
    assign containerWidth = ''
  endif

  if section.settings.VEDA_slider_5 >= 0
    assign settingGap = section.settings.VEDA_slider_5 | divided_by: 2.0

    assign settingGapVertical = section.settings.VEDA_slider_5
  else
    assign settingGap = ''

    assign settingGapVertical = ''
  endif

  assign colGap = 'margin-top: ' | append: settingGapVertical | append: 'px; padding-left: ' | append: settingGap | append: 'px; padding-right: ' | append: settingGap | append: 'px;'

  assign rowGap = 'margin-top: -' | append: settingGapVertical | append: 'px; margin-left: -' | append: settingGap | append: 'px; margin-right: -' | append: settingGap | append: 'px;'

  assign sectionSeparator = ''

  if section.settings.VEDA_switch_5
    if section.settings.VEDA_slider_4
      assign sectionSeparator = sectionSeparator | append: 'border-top: ' | append: section.settings.VEDA_slider_4 | append: 'px solid ' | append: '#f3f3f3' | append: ';'
    endif

    if section.settings.VEDA_slider_3
      assign sectionSeparator = sectionSeparator | append: 'border-bottom: ' | append: section.settings.VEDA_slider_3 | append: 'px solid ' | append: '#f3f3f3' | append: ';'
    endif
  endif

  if colGap and rowGap and builderMode and envatoMode
  endif
%}
{% comment %}<KẾT_THÚC_TÁCH_FILE>{% endcomment %}

{% liquid
  assign _alt = false

  assign _style = false

  assign _placeholder = false

  assign _placeholder_class = false

  assign _style = false


  assign _class = false

  assign _component = false

  assign _defaul_aspect_ratio = false


  assign _image_border_radius = false
%}

{% capture Empty %}
  <div class="veda-empty maw:600px m:auto p:60px_30px d:flex fld:column ai:center ta:center">
    <h3 class="fw:600">{{ 'veda.general.empty.title' | t }}</h3>
    <div>{{ 'veda.general.empty.text' | t }}</div>
  </div>
{% endcapture %}

{% capture CloseFilterMobile %}
  <div
    class="pos:absolute t:0 l:0 d:none@md w:100% h:100% bgc:color-gray9 trs:opacity_0.3s op:0 z:-1"
    data-modal-active-class="pe:auto op:0.5!"
    data-modal-class="pe:none"
  ></div>
  <div class="w:100% h:60px d:flex jc:flex-end ai:center d:none@md">
    <span
      class="btn-close-filter-js mr:15px d:flex jc:center ai:center w:36px h:36px fz:18px c:color-light c:color-light|h bd:1px_solid_color-light bdrs:50% trf:scale(0) trs:0.3s cur:pointer"
      data-modal-active-class="trf:scale(1)!"
      data-modal-class=""
    >
      <i class="fal fa-times"></i>
    </span>
  </div>
{% endcapture %}

{% capture CloseFilterFixed %}
  <span class="btn-close-filter-js pos:absolute t:0 r:0 d:flex jc:center ai:center w:36px h:36px fz:22px c:color-gray6 c:color-primary|h trs:0.3s cur:pointer">
    <i class="fal fa-times"></i>
  </span>
{% endcapture %}

{% capture SideBar %}
  {% liquid
    assign collection_data = collections

    assign _products = collection.products

    assign my_collection_sort = collection.sort_options

    assign min_price = 0

    assign collection_vendors = collection.all_vendors

    assign collection_tags = collection.all_tags

    assign collection_types = collection.all_types

    assign instockCount = collection.products | where: 'available', true | size

    assign outofstockCount = 1
  %}

  <div
    {% if section.settings.filter_variant == 'top_left_fixed' %}
      class="filter-container-js pos:fixed t:0 l:0 z:99 w:300px h:100vh m:0! p:0! trs:0.3s mah:none! v:visible! ov:visible!"
      data-modal-active-class="trf:translateX(0)! bxsh:0_0_20px_color-gray9.1"
      data-modal-class="trf:translateX(-100%)"
    {% elsif section.settings.filter_variant == 'top_right_fixed' %}
      class="filter-container-js pos:fixed t:0 r:0 z:99 w:300px h:100vh m:0! p:0! trs:0.3s mah:none! v:visible! ov:visible!"
      data-modal-active-class="trf:translateX(0)! bxsh:0_0_20px_color-gray9.1"
      data-modal-class="trf:translateX(100%)"
    {% elsif section.settings.filter_variant == 'horizontal' %}
      class="filter-container-js pos:relative col-xs-12 col-sm-12 col-md-12 col-lg-12 z:99"
      data-modal-active-class="pe:auto!"
      data-modal-class="pos:fixed! t:0! l:0! mt:0! p:0! w:100%! h:100%! pe:none mah:none! v:visible! ov:visible!"
    {% else %}
      class="filter-container-js pos:relative col-xs-12 col-sm-12 col-md-12 col-lg-3 z:99 w:var(--filter-width)@lg mah:none! v:visible! ov:visible!"
      data-modal-active-class="pe:auto!"
      data-modal-class="pos:fixed! t:0! l:0! mt:0! p:0! w:100%! h:100%! pe:none"
    {% endif %}
    {% if section.settings.VEDA_slider_5 or section.settings.VEDA_slider_5 == 0 %}
      style="{{ colGap }}"
    {% endif %}
  >
    {% if section.settings.filter_variant != 'horizontal' %}
      {% if section.settings.filter_variant == 'top_left_fixed'
        or section.settings.filter_variant == 'top_right_fixed'
      %}
        {{ CloseFilterFixed }}
      {% else %}
        {{ CloseFilterMobile }}
      {% endif %}
    {% endif %}

    <div
      {% if section.settings.filter_variant == 'top_left_fixed' %}
        class="w:100% h:100% p:20px bdr:1px_solid_color-gray3 bgc:color-light veda-scrollbar"
      {% elsif section.settings.filter_variant == 'top_right_fixed' %}
        class="w:100% h:100% p:20px bdl:1px_solid_color-gray3 bgc:color-light veda-scrollbar"
      {% else %}
        {% if section.settings.filter_variant == 'horizontal' %}
          class="w:100%"
        {% else %}
          class="w:100% h:calc(100vh_-_60px) h:fit-content@md! bgc:color-light@+md p:15px@+md trs:transform_0.3s trf:translateY(100%)@+md ov:visible@md! veda-scrollbar"
        {% endif %}
        data-modal-active-class="trf:translateY(0)!"
        data-modal-class=""
      {% endif %}
      data-css="{{ null }}"
    >
      <div class="w:100% h:fit-content">
        <form class="filter-form-js">
          <div class="popover-wrapper-js w:100% pos:relative"></div>
          <div
            {% if section.settings.filter_variant == 'horizontal' %}
              class="d:flex ai:center fxw:wrap@md ovx:auto! ovy:hidden!"
            {% endif %}
          >
            {% capture Notify %}
              {% if builderMode %}
                <div class="mb:30px">
                  <div class="d:flex fz:14px p:10px bgc:color-warning.1 c:color-warning bd:1px_solid_color-warning">
                    <span><i class="far fa-database mr:10px"></i></span>
                    <div>The data below may be fake. Please set it up at Apps > Search & Discovery > Filters.</div>
                  </div>
                </div>
              {% endif %}
            {% endcapture %}
            {% comment %}CẢNH BÁO: Các field settings đã bị cắt vợi đi để pass qua shopify theme check => Các item sẽ trở thành y hệt nhau về một số thứ => Check code để fix css sao cho giống với theme demo => Ví dụ mỗi item đều có field color picker để setup background. Item 1 màu xanh, Item 2 màu đỏ => Sync ở chế độ cắt vợi đi thì cả 2 Item sẽ thành màu xanh => Sai với theme demo => Cần fix{% endcomment %}

            {% for block in section.blocks %}
              {% assign filter = block.settings %}

              {% liquid
                if section.settings.filter_variant == 'horizontal'
                  assign boxSpaceClass = 'pos:relative'
                else
                  assign boxSpaceClass = 'mb:30px'
                endif
              %}
              {% if filter.filters___enable %}
                {% liquid
                  assign clearClass = 'filter-clear-js c:color-gray6 c:color-primary|h trs:0.3s cur:pointer d:none'

                  assign clearText = 'veda.sections.collection_template.clear' | t
                %}

                {% capture FakeBrand %}
                  {% for brand in collection_vendors %}
                    {% liquid
                      assign count = collection.products | where: 'vendor', brand | size
                    %}
                    <label
                      {% if count == 0 %}
                        class="veda-filter-label pos:relative d:flex ai:center op:0.5 pe:none td:line-through"
                      {% else %}
                        class="veda-filter-label pos:relative d:flex ai:center"
                      {% endif %}
                    >
                      <input
                        class="veda-filter-checkbox pos:absolute v:hidden"
                        type="checkbox"
                        name="filter.p.vendor"
                        value="{{ brand }}"
                        data-label="{{ brand }}"
                      >
                      {% if filter.VEDA_switch_1 %}
                        <div
                          class="veda-filter-icon-check mr:7px"
                          {% if null %}
                            style="border-color: {{ null }}"
                          {% endif %}
                        ></div>
                      {% endif %}
                      <span
                        class="d:block w:100% fz:14px cur:pointer p:5px_0"
                        data-css="{{ null }}"
                      >
                        {{ brand }}
                        <span
                          data-type="brands-{{ brand }}"
                          {% if filter.VEDA_select_1 == 'right' %}
                            class="veda-filter-count fl:right"
                          {% else %}
                            class="veda-filter-count"
                          {% endif %}
                          >({{ count }})
                        </span>
                      </span>
                    </label>
                  {% endfor %}
                {% endcapture %}

                {% capture FakeTag %}
                  {% for tag in collection_tags %}
                    <label
                      {% if count == 0 %}
                        class="veda-filter-label pos:relative d:inline-block cur:pointer mr:5px mb:5px op:0.5 pe:none td:line-through"
                      {% else %}
                        class="veda-filter-label pos:relative d:inline-block cur:pointer mr:5px mb:5px"
                      {% endif %}
                    >
                      <input
                        class="veda-filter-tag pos:absolute v:hidden"
                        type="checkbox"
                        name="filter.p.tag"
                        value="{{ tag }}"
                        data-label="{{ tag }}"
                      >
                      <div
                        class="p:3px_8px ta:center fz:14px! c:color-primary|h bd:1px_solid_color-gray3 trs:0.3s"
                        data-css="{{ null }}"
                      >
                        {{ tag }}
                        {% if count %}
                          <span data-type="tags-{{ tag }}" class="veda-filter-count">({{ count }})</span>
                        {% endif %}
                      </div>
                    </label>
                  {% endfor %}
                {% endcapture %}

                {% capture FakeProductType %}
                  {% for itemType in collection_types %}
                    {% liquid
                      assign count = collection.products | where: 'type', itemType | size
                    %}
                    <label
                      {% if count == 0 %}
                        class="veda-filter-label pos:relative d:flex ai:center op:0.5 pe:none td:line-through"
                      {% else %}
                        class="veda-filter-label pos:relative d:flex ai:center"
                      {% endif %}
                    >
                      <input
                        class="veda-filter-checkbox pos:absolute v:hidden"
                        type="checkbox"
                        name="filter.p.product_type"
                        value="{{ itemType }}"
                        data-label="{{ itemType }}"
                      >
                      {% if filter.VEDA_switch_1 %}
                        <div
                          class="veda-filter-icon-check mr:7px"
                          {% if null %}
                            style="border-color: {{ null }}"
                          {% endif %}
                        ></div>
                      {% endif %}
                      <span
                        class="d:block w:100% fz:14px cur:pointer p:5px_0"
                        data-css="{{ null }}"
                      >
                        {{ itemType }}
                        <span
                          data-type="type-{{ itemType }}"
                          {% if filter.VEDA_select_1 == 'right' %}
                            class="veda-filter-count fl:right"
                          {% else %}
                            class="veda-filter-count"
                          {% endif %}
                          >({{ count }})
                        </span>
                      </span>
                    </label>
                  {% endfor %}
                {% endcapture %}

                {% capture FakeAvailability %}
                  <label
                    {% if instockCount == 0 %}
                      class="veda-filter-label pos:relative d:flex ai:center op:0.5 pe:none td:line-through"
                    {% else %}
                      class="veda-filter-label pos:relative d:flex ai:center"
                    {% endif %}
                  >
                    <input
                      class="veda-filter-radio pos:absolute v:hidden"
                      type="radio"
                      name="filter.v.availability"
                      value="1"
                      data-label="{{ 'veda.products.product.instock' | t }}"
                    >
                    {% if filter.VEDA_switch_1 %}
                      <div
                        class="veda-filter-icon-check mr:7px"
                        {% if null %}
                          style="border-color: {{ null }}"
                        {% endif %}
                      ></div>
                    {% endif %}
                    <span
                      class="d:block w:100% fz:14px cur:pointer p:5px_0"
                      data-css="{{ null }}"
                    >
                      {{ 'veda.products.product.instock' | t }}
                      <span
                        data-type="instock"
                        {% if filter.VEDA_select_1 == 'right' %}
                          class="veda-filter-count fl:right"
                        {% else %}
                          class="veda-filter-count"
                        {% endif %}
                        >({{ instockCount }})
                      </span>
                    </span>
                  </label>
                  <label
                    {% if outofstockCount == 0 %}
                      class="veda-filter-label pos:relative d:flex ai:center op:0.5 pe:none td:line-through"
                    {% else %}
                      class="veda-filter-label pos:relative d:flex ai:center"
                    {% endif %}
                  >
                    <input
                      class="veda-filter-radio pos:absolute v:hidden"
                      type="radio"
                      name="filter.v.availability"
                      value="0"
                      data-label="{{ 'veda.products.product.outofstock' | t }}"
                    >
                    {% if filter.VEDA_switch_1 %}
                      <div
                        class="veda-filter-icon-check mr:7px"
                        {% if null %}
                          style="border-color: {{ null }}"
                        {% endif %}
                      ></div>
                    {% endif %}
                    <span
                      class="d:block w:100% fz:14px cur:pointer p:5px_0"
                      data-css="{{ null }}"
                    >
                      {{ 'veda.products.product.outofstock' | t }}
                      <span
                        data-type="outofstock"
                        {% if filter.VEDA_select_1 == 'right' %}
                          class="veda-filter-count fl:right"
                        {% else %}
                          class="veda-filter-count"
                        {% endif %}
                        >({{ outofstockCount }})
                      </span>
                    </span>
                  </label>
                {% endcapture %}

                {% case block.type %}
                  {% when 'filters_banner' %}
                    <div class="{{ boxSpaceClass }}" data-css="{{ null }}">
                      <div
                        {% if section.settings.filter_variant == 'horizontal' %}
                          class="d:flex ai:center cur:pointer us:none mih:36px mr:pfs(20px,30px)"
                          data-modal-button-id="{{ block.type }}-{{ forloop.index }}"
                          data-modal-active-class="trf:rotate(180deg)*i"
                        {% else %}
                          class="d:flex ai:center jc:space-between mb:5px"
                        {% endif %}
                      >
                        {% if section.settings.filter_variant == 'horizontal' %}
                          <h2 class="fz:15px fw:400 m:0 d:flex ai:center" data-css="{{ null }}">
                            <span>{{ filter.filters___title }}</span>
                            <div class="ml:5px">
                              <i class="far fa-angle-down trs:0.3s"></i>
                            </div>
                          </h2>
                        {% else %}
                          <h2 class="fz:16px fw:500 m:0" data-css="{{ null }}">
                            {{ filter.filters___title }}
                          </h2>
                        {% endif %}
                      </div>
                      <div
                        {% if section.settings.filter_variant == 'horizontal' %}
                          class="pos:absolute l:0@+md! w:100%@+md! z:999 mt:10px bgc:color-light bd:1px_solid_color-gray3 w:260px bdrs:4px bxsh:0_6px_15px_color-gray9.1 v:hidden op:0 trs:transform_0.3s,visibility_0.3s,opacity_0.3s trf:translateY(30px)"
                          data-modal-content-id="{{ block.type }}-{{ forloop.index }}"
                          data-modal-active-class="z:99999 v:visible! op:1! trf:translateY(0)!"
                        {% endif %}
                      >
                        <a
                          href="{{ filter.filters___banner_link }}"
                          class="pos:relative d:block"
                          data-css="{{ null }}"
                        >
                          {% comment %}<BẮT_ĐẦU_TÁCH_FILE_2:[section,backgroundFixed,backgroundColor,bgImage,backgroundImage,sectionPadding,containerType,containerGap,containerWidth,settingGap,settingGapVertical,colGap,rowGap,sectionSeparator,_alt,_style,_placeholder,_placeholder_class,_enable,_class,_component,_defaul_aspect_ratio,_aspect_ratio,_image_border_radius,collection_data,_products,my_collection_sort,min_price,collection_vendors,collection_tags,collection_types,instockCount,outofstockCount,boxSpaceClass,clearClass,clearText,count,filter]>{% endcomment %}
                          {% if filter.filters___banner_image.src != blank %}
                            <img
                              class="veda-lazyload {{ _class }}"
                              src="{{ filter.filters___banner_image.src | image_url: width: 10 }}"
                              draggable="false"
                              {% if _alt %}
                                alt="{{ _alt }}"
                              {% else %}
                                alt="image"
                              {% endif %}
                              {% if filter.filters___banner_image.width %}
                                data-intrinsic-width="{{ filter.filters___banner_image.width }}"
                                width="{{ filter.filters___banner_image.width }}"
                              {% endif %}
                              {% if filter.filters___banner_image.height %}
                                height="{{ filter.filters___banner_image.height }}"
                                data-intrinsic-height="{{ filter.filters___banner_image.height }}"
                              {% endif %}
                              {% if _style %}
                                style="{{ _style }}"
                              {% endif %}
                              loading="lazy"
                            >
                          {% else %}
                            {% liquid
                              assign pl = 'lifestyle-1'

                              if _placeholder
                                assign pl = _placeholder
                              endif

                              assign placeholder_class = 'veda-placeholder-svg bgc:color-gray2 ' | append: _class

                              if _placeholder_class
                                assign placeholder_class = 'veda-placeholder-svg bgc:color-gray2 ' | append: _placeholder_class
                              endif
                            %}
                            {{ pl | placeholder_svg_tag: placeholder_class }}
                          {% endif %}
                          {% comment %}<KẾT_THÚC_TÁCH_FILE>{% endcomment %}
                        </a>
                      </div>
                    </div>
                  {% when 'filters_collections' %}
                    <div class="{{ boxSpaceClass }}" data-css="{{ null }}">
                      <div
                        {% if section.settings.filter_variant == 'horizontal' %}
                          class="d:flex ai:center cur:pointer us:none mih:36px mr:pfs(20px,30px)"
                          data-modal-button-id="{{ block.type }}-{{ forloop.index }}"
                          data-modal-active-class="trf:rotate(180deg)*i"
                        {% else %}
                          class="d:flex ai:center jc:space-between mb:5px"
                        {% endif %}
                      >
                        {% if section.settings.filter_variant == 'horizontal' %}
                          <h2 class="fz:15px fw:400 m:0 d:flex ai:center" data-css="{{ null }}">
                            <span>{{ 'veda.general.collections' | t }}</span>
                            <div class="ml:5px"><i class="far fa-angle-down trs:0.3s"></i></div>
                          </h2>
                        {% else %}
                          <h2 class="fz:16px fw:500 m:0" data-css="{{ null }}">
                            {{ 'veda.general.collections' | t }}
                          </h2>
                          <div data-css="{{ null }}" class="{{ clearClass }}" data-name="filter.v.collection">
                            {{ clearText }}
                          </div>
                        {% endif %}
                      </div>
                      <div
                        {% if section.settings.filter_variant == 'horizontal' %}
                          class="veda-scrollbar veda-scrollbar--2 mah:220px pos:absolute l:0@+md! w:100%@+md! z:999 mt:10px bgc:color-light bd:1px_solid_color-gray3 pl:15px pb:10px pr:15px w:260px bdrs:4px bxsh:0_6px_15px_color-gray9.1 v:hidden op:0 trs:transform_0.3s,visibility_0.3s,opacity_0.3s trf:translateY(30px)"
                          data-modal-content-id="{{ block.type }}-{{ forloop.index }}"
                          data-modal-active-class="z:99999 v:visible! op:1! trf:translateY(0)!"
                        {% else %}
                          class="veda-scrollbar veda-scrollbar--2 mah:220px"
                        {% endif %}
                      >
                        {% if section.settings.filter_variant == 'horizontal' %}
                          <div class="d:flex ai:center jc:space-between p:10px_15px pr:10px! ml:-15px mr:-15px mb:10px bdb:1px_solid_color-gray3">
                            <div>
                              <div
                                data-css="{{ null }}"
                                class="td:underline {{ clearClass }}"
                                data-name="filter.v.collection"
                              >
                                {{ clearText }}
                              </div>
                            </div>
                            <div
                              data-modal-button-close-id="{{ block.type }}-{{ forloop.index }}"
                              class="fz:16px cur:pointer w:20px h:20px d:flex ai:center jc:center"
                            >
                              <i class="fal fa-times"></i>
                            </div>
                          </div>
                        {% endif %}
                        {% comment %}CẢNH BÁO: Các field settings đã bị cắt vợi đi để pass qua shopify theme check => Các item sẽ trở thành y hệt nhau về một số thứ => Check code để fix css sao cho giống với theme demo => Ví dụ mỗi item đều có field color picker để setup background. Item 1 màu xanh, Item 2 màu đỏ => Sync ở chế độ cắt vợi đi thì cả 2 Item sẽ thành màu xanh => Sai với theme demo => Cần fix{% endcomment %}

                        {% for block in section.blocks %}
                          {% if block.type == 'collection_filter' %}
                            {% assign item = block.settings %}

                            {% liquid
                              assign collection_key = item.collection_filter___link | replace: '/collections/', ''

                              assign handles = collection_data | map: 'handle'

                              assign count = collection_data[collection_key].all_products_count
                            %}
                            <label
                              {% if count == 0 %}
                                class="veda-filter-label pos:relative d:flex ai:center op:0.5 pe:none td:line-through"
                              {% else %}
                                class="veda-filter-label pos:relative d:flex ai:center"
                              {% endif %}
                            >
