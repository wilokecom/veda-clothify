        <div class="d:none@md mt:30px">
          {{ ViewAllBase }}
        </div>
      {% endif %}
    {% endcapture %}

    {% if 'bottom' == 'top' %}
      {{ ViewAllTop }}
    {% else %}
      {{ ViewAllBottom }}
    {% endif %}
    {% comment %}<KẾT_THÚC_TÁCH_FILE>{% endcomment %}
  </div>
  {% if 'none' == 'video' and null %}
    <div data-veda-video="{{ null }}"></div>
  {% endif %}
  {% if null %}
