{% capture Header %}
  <div
    class="d:flex ai:center jc:space-between fxw:wrap p:20px_65px@sm p:20px_10px bdb:1px_solid_color-gray3 veda-password-header-js"
    data-css="{{ null }}"
  >
    <div class="fz:20px fz:800 ff:font-secondary c:color-gray9 tt:uppercase" data-css="{{ null }}">
      {{ shop.name }}
    </div>
    {% if shop.password_message != blank %}
      <div class="password-content">
        {{ shop.password_message }}
      </div>
    {% endif %}
    <div
      class="toggle-password-form-js fz:14px fw:500 c:color-gray6 td:underline us:none cur:pointer"
      data-css="{{ null }}"
    >
      {{ section.settings.VEDA_text_3 }}
    </div>
  </div>
{% endcapture %}

{% capture Footer %}
  <div
    class="d:flex fld:column ai:center p:30px_10px bdt:1px_solid_color-gray3 veda-password-footer-js"
    data-css="{{ null }}"
  >
    <div>
      {% comment %}<BẮT_ĐẦU_TÁCH_FILE_5:[section,backgroundFixed,backgroundColor,bgImage,backgroundImage,sectionPadding,containerType,containerGap,containerWidth,settingGap,settingGapVertical,colGap,rowGap,sectionSeparator,_component,_class,_data_confirm_message,_data_target,_aria_label,_data_index,_data_form_active,_form,_name,_id,_tooltip_position,_tooltip_text,_data_button_video_id,_gap,_test,_icon_class,default_class,default_for_bottom_class,variant_class,align,headingDividerPadding]>{% endcomment %}
      {% liquid
        assign gap = 10

        if _gap
          assign gap = _gap | divided_by: 2
        endif
      %}

      <div class="d:flex fxw:wrap ai:center" style="margin-left: -{{ gap }}px; margin-right: -{{ gap }}px">
        {% liquid
          assign share_arr = section.settings.share___share_type | split: ','
        %}
        {% for item in share_arr %}
          {% case item %}
            {% when 'facebook' %}
              <a
                target="_blank"
                href="https://www.facebook.com/sharer/sharer.php?u={{ shop.url | append: shop.url }}"
                component="{{ _component }}"
              >
                <span
                  class="fz:16px c:color-gray9 trf:translateY(-5px)|h trs:0.3s"
                  style="padding-left: {{ gap }}px; padding-right: {{ gap }}px;"
                  data-css="{{ null }}"
                  {% if _tooltip_position %}
                    data-tooltip-position="{{ _tooltip_position }}"
                  {% else %}
                    data-tooltip-position="top"
                  {% endif %}
                  data-tooltip-text="{{ 'veda.general.social.alt_text.share_on_facebook' | t }}"
                >
                  <i class="fab fa-facebook"></i>
                </span>
              </a>
            {% when 'twitter' %}
              <a
                target="_blank"
                href="https://twitter.com/intent/tweet?url={{ shop.url | append: shop.url }}"
                component="{{ _component }}"
              >
                <span
                  class="fz:16px c:color-gray9 trf:translateY(-5px)|h trs:0.3s"
                  style="padding-left: {{ gap }}px; padding-right: {{ gap }}px;"
                  data-css="{{ null }}"
                  {% if _tooltip_position %}
                    data-tooltip-position="{{ _tooltip_position }}"
                  {% else %}
                    data-tooltip-position="top"
                  {% endif %}
                  data-tooltip-text="{{ 'veda.general.social.alt_text.share_on_twitter' | t }}"
                >
                  <i class="fab fa-twitter"></i>
                </span>
              </a>
            {% when 'messenger' %}
              <a
                target="_blank"
                href="https://www.facebook.com/dialog/send?app_id=1217981644879628&link={{ shop.url | append: shop.url }}?utm_source=ig_web_button_share_sheet&redirect_uri={{ shop.url | append: shop.url }}?utm_source=ig_web_button_share_sheet"
                component="{{ _component }}"
              >
                <span
                  class="fz:16px c:color-gray9 trf:translateY(-5px)|h trs:0.3s"
                  style="padding-left: {{ gap }}px; padding-right: {{ gap }}px;"
                  data-css="{{ null }}"
                  {% if _tooltip_position %}
                    data-tooltip-position="{{ _tooltip_position }}"
                  {% else %}
                    data-tooltip-position="top"
                  {% endif %}
                  data-tooltip-text="{{ 'veda.general.social.alt_text.share_on_messager' | t }}"
                >
                  <i class="fab fa-facebook-messenger"></i>
                </span>
              </a>
            {% when 'pinterest' %}
              <a
                target="_blank"
                href="https://pinterest.com/pin/create/button/?url={{ shop.url | append: shop.url }}"
                component="{{ _component }}"
              >
                <span
                  class="fz:16px c:color-gray9 trf:translateY(-5px)|h trs:0.3s"
                  style="padding-left: {{ gap }}px; padding-right: {{ gap }}px;"
                  data-css="{{ null }}"
                  {% if _tooltip_position %}
                    data-tooltip-position="{{ _tooltip_position }}"
                  {% else %}
                    data-tooltip-position="top"
                  {% endif %}
                  data-tooltip-text="{{ 'veda.general.social.alt_text.share_on_pinterest' | t }}"
                >
                  <i class="fab fa-pinterest"></i>
                </span>
              </a>
            {% when 'telegram' %}
              <a
                target="_blank"
                href="https://telegram.me/share/url?url={{ shop.url | append: shop.url }}"
                component="{{ _component }}"
              >
                <span
                  class="fz:16px c:color-gray9 trf:translateY(-5px)|h trs:0.3s"
                  style="padding-left: {{ gap }}px; padding-right: {{ gap }}px;"
                  data-css="{{ null }}"
                  {% if _tooltip_position %}
                    data-tooltip-position="{{ _tooltip_position }}"
                  {% else %}
                    data-tooltip-position="top"
                  {% endif %}
                  data-tooltip-text="{{ 'veda.general.social.alt_text.share_on_telegram' | t }}"
                >
                  <i class="fab fa-telegram"></i>
                </span>
              </a>
            {% when 'email' %}
              <a
                target="_blank"
                href="mailto:?subject={{ shop.url | append: shop.url }}"
                component="{{ _component }}"
              >
                <span
                  class="fz:16px c:color-gray9 trf:translateY(-5px)|h trs:0.3s"
                  style="padding-left: {{ gap }}px; padding-right: {{ gap }}px;"
                  data-css="{{ null }}"
                  {% if _tooltip_position %}
                    data-tooltip-position="{{ _tooltip_position }}"
                  {% else %}
                    data-tooltip-position="top"
                  {% endif %}
                  data-tooltip-text="{{ 'veda.general.social.alt_text.share_on_email' | t }}"
                >
                  <i class="fas fa-envelope"></i>
                </span>
              </a>
            {% else %}

          {% endcase %}
        {% endfor %}
      </div>
      {% comment %}<KẾT_THÚC_TÁCH_FILE>{% endcomment %}
    </div>
    <div
      class="d:flex ai:center jc:center fxw:wrap mt:20px fz:13px c:color-gray8 ta:center"
      data-css="{{ null }}"
    >
      {{ 'veda.general.password_page.powered_by_shopify_html' | t: shopify: shopify }}
    </div>
    <div
      class="mt:15px fz:13px! fw:400 c:color-gray7 fz:13px*a td:underline*a trs:0.3s*a ta:center"
      data-css="{{ null }}"
    >
      {{ 'veda.general.password_page.admin_link_html' | t }}
    </div>
  </div>
{% endcapture %}

